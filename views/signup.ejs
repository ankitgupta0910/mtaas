                    	<div class="form-group" ng-class="{'has-error': (signupForm.submitted || signupForm.pri_lang.$touched || signupForm.pri_lang.$dirty) && signupForm.pri_lang.$invalid,					'has-success': (signupForm.pri_lang.$touched || signupForm.pri_lang.$dirty) && signupForm.pri_lang.$valid}">
						<select name="pri_lang" class="form-control" ng-model="user.pri_lang" required>
				        <option label="Primary Language" selected="" disabled=""></option>
                    	<option value="English">English</option>
	                 	<option value="Chinese">Chinese</option>  		
	                 	<option value="French">French</option>
	                 	<option value="German">German</option>
	                 	<option value="Latin">Latin</option>
	                 	<option value="Portuguese">Portuguese</option>
	                 	<option value="Russian">Russian</option>
	                 	<option value="Japanese">Japanese</option>
	                 	<option value="Korean">Korean</option>
	                 	<option value="Hindi">Hindi</option>
                    	</select>
                    	<span class=" help-block glyphicon glyphicon-exclamation-sign" ng-show="(signupForm.submitted || signupForm.pri_lang.$touched || signupForm.pri_lang.$dirty) && signupForm.pri_lang.$error.required">Primary Language Required</span>
                    	</div>
                    	
                    	<div class="form-group" ng-class="{'has-error': (signupForm.submitted || signupForm.sec_lang.$touched || signupForm.sec_lang.$dirty) && signupForm.sec_lang.$invalid,					'has-success': (signupForm.sec_lang.$touched || signupForm.sec_lang.$dirty) && signupForm.sec_lang.$valid}">
						<select name="sec_lang" class="form-control" ng-model="user.sec_lang" required>
				        <option label="Secondary Language" selected="" disabled=""></option>
                    	<option value="English">English</option>
                    	<option value="Chinese">Chinese</option>
                    	</select>
                    	<span class=" help-block glyphicon glyphicon-exclamation-sign" ng-show="(signupForm.submitted || signupForm.sec_lang.$touched || signupForm.sec_lang.$dirty) && signupForm.sec_lang.$error.required">Secondary Language Required</span>
                    	</div>  

                    	<div class="form-group" ng-show="extr">
                    	<label>Testing Platform:</label>
                    	<div class="checkbox">
                    		<label><input type="checkbox"  ng-model="user.tr_p0" name="plat" ng-true-value="'Android'">Android</label>
						</div>
						<div class="checkbox">
							<label><input type="checkbox" ng-model="user.tr_p1" name="plat" ng-true-value="'IOS'">IOS</label>
						</div>
						<div class="checkbox">
							<label><input type="checkbox" ng-model="user.tr_p2" name="plat"  ng-true-value="'Windows'">Window</label>
						</div>
						<span class=" help-block glyphicon glyphicon-exclamation-sign" ng-show="plat1">Platform Type Required</span>
                    	</div>
                    	
                    	<div class="form-group" ng-show="extr">
						<label>Testing Type:</label>
						<div class="checkbox">
							<label><input type="checkbox" ng-model="user.tr_t0" name="tr_t0" ng-true-value="'Functional Testing'">
							 Functional Testing</label>
						</div>
						<div class="checkbox">
							<label><input type="checkbox" ng-model="user.tr_t1" name="tr_t1" ng-true-value="'Laboratory Testing'">
							Laboratory Testing</label>
						</div>
						<div class="checkbox">
							<label><input type="checkbox" ng-model="user.tr_t2" name="tr_t2" ng-true-value="'Performance Testing'">
							Performance Testing</label>
						</div>
						<div class="checkbox">
							<label><input type="checkbox" ng-model="user.tr_t3" name="tr_t3" ng-true-value="'Memory Leakage Testing'">
							Memory Leakage Testing</label>
						</div>
						<div class="checkbox">
							<label><input type="checkbox" ng-model="user.tr_t4" name="tr_t4" ng-true-value="'Interrupt Testing'">
							Interrupt Testing</label>
                    	</div>
                    	<span class=" help-block glyphicon glyphicon-exclamation-sign" ng-show="type1">Testing Type Required</span>
                    	</div>



                     	<input type="submit" ng-click="signupForm.submitted = true" class="btn btn-default btn-primary" style="width:50%;margin:0 0 0 25%">
						</form>
	</div>

</div>
</body>

<script type="text/javascript">

var app = angular.module('mtaas', []);

app.controller('mtaas', ['$scope','$http','$window',function ($scope,$http,$window)  {
	
	$scope.ustp = function()
	{

		if ($scope.user["usertype"] === "T")
			{
			$scope.extr = true;
			}
		else
			{
			$scope.extr = false;
			}
	}
	
	$scope.signup = function()
	{
		if($scope.user["tr_p0"] === "Android" || $scope.user["tr_p1"] === "Ios" || $scope.user["tr_p2"] === "Windows")
			{
			if($scope.user["tr_t0"] === "Functional Testing" || $scope.user["tr_t1"] === "Laboratory Testing" || $scope.user["tr_t2"] === "Performance Testing" || $scope.user["tr_t3"] === "Memory Leakage Testing" || $scope.user["tr_t4"] === "Interrupt Testing")
				{
				$http({
		  	  		method: 'POST',
		  	 		url: '/signup',
		  	 		data: {"user":$scope.user}
		    	 }).
		    	then(function(response)
		    	{
		    		alert()
		    		$window.location.href = '/';
		    	})
				}
			else
				{
				$scope.plat1=false;
				$scope.type1=true;
				}
			}
		else
			{
			$scope.plat1=true;
			}
	}	
	
	$scope.signin = function (){
		$window.location.href = '/';
	}
	
}]);

</script>
</html>